{
  "name": "gatsby-plugin-plugin-versions",
  "version": "0.0.0",
  "description": "Gatsby plugin that adds a plugins.json with name and versions to deploy directory",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "watch": "babel -w src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin"
  ],
  "author": "Dustin Schau <dustin@gatsbyjs.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "babel-preset-gatsby-package": "^0.1.3",
    "cross-env": "^5.0.5"
  },
  "peerDependencies": {
    "gatsby": "^2.0.0"
  },
  "dependencies": {
    "fs-extra": "^7.0.1"
  }
}
